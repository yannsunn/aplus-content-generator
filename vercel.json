{
  "version": 2,
  "functions": {
    "api/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend.html"
    },
    {
      "source": "/index.html",
      "destination": "/frontend.html"
    }
  ],
  "github": {
    "silent": true
  },
  "public": true
}